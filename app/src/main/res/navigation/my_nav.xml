<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/recipesFragment">

    <fragment
        android:id="@+id/recipesFragment"
        android:name="com.dina.elcg.myreciepes.ui.fragments.recipeis.RecipesFragment"
        android:label="Recipes"
        tools:layout="@layout/fragment_recipes" >
        <action
            android:id="@+id/action_recipesFragment_to_bottomSheetFragment2"
            app:destination="@id/bottomSheetFragment2" />
        <argument
            android:name="backFromBottomSheet"
            app:argType="boolean"
            android:defaultValue="false" />
    </fragment>
    <fragment
        android:id="@+id/favoritesFragment"
        android:name="com.dina.elcg.myreciepes.ui.fragments.favorities.FavoritesFragment"
        android:label="Favorites"
        tools:layout="@layout/fragment_favorites" />
    <fragment
        android:id="@+id/jokeFragment"
        android:name="com.dina.elcg.myreciepes.ui.fragments.jokes.JokeFragment"
        android:label="Joke"
        tools:layout="@layout/fragment_joke" />
    <dialog
        android:id="@+id/bottomSheetFragment2"
        android:name="com.dina.elcg.myreciepes.ui.fragments.recipeis.bottomsheet.BottomSheetFragment"
        android:label="fragment_bottom_sheet"
        tools:layout="@layout/fragment_bottom_sheet" >
        <action
            android:id="@+id/action_bottomSheetFragment2_to_recipesFragment"
            app:destination="@id/recipesFragment" />
    </dialog>
</navigation>